{
    "manifest_version": 2,
    "name": "PhishNet",
    "version": "1.0",
  
    "description": "Warns users of suspiscious emails that appear to be phishing.",

  
    "content_scripts": [
      {
        "matches": ["*://*.mail.proton.me/*", "*://*.mail.google.com/*", "*://*.mail.yahoo.com/*",
		    "*://*.mail.com/*", "*://*.mail.aol.com/*", "*://*.outlook.live.com/*", "*://*.icloud.com/mail/*"],
        "js": ["phishnet.js"]
      }
    ]
  }
  
